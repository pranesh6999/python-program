print("what operation do u want")
operator=input('enter either = +,-,%,/,*:')
n1=float(input( 'enter the value :'))
n2=float(input ('enter the value :'))
if operator=='+':
    print(n1,operator,n2,'=',n1+n2)
elif operator =='-':
    print(n1,operator,n2,'=',n1-n2)
elif operator =='/':
    print(n1,operator,n2,'=',n1/n2)
elif operator =='%':
    print(n1,operator,n2,'=',n1%n2)
elif operator =='*':
    print(n1,operator,n2,'=',n1*n2)
else:
    print('wrong values')
